{
  "name": "repetierer",
  "version": "5.0.0",
  "description": "(:",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonas-Luetolf/Repetierer.git"
  },
  "main": "app.js",
  "scripts": {
    "start": "electron . ",
    "build": "electron-builder",
    "release": "electron-builder -p=always"
  },
  "build": {
    "appId": "net.srpnt3.repetierer",
    "productName": "Repetierer",
    "directories": {
      "output": "./builds"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./public/icons/icon.ico",
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "installerIcon": "./public/icons/icon.ico",
      "uninstallerIcon": "./public/icons/icon.ico",
      "installerSidebar": "./public/images/sidebar.bmp",
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": true,
      "artifactName": "Repetierer ${version}.${ext}"
    }
  },
  "author": "srpnt3, Jonas-Luetolf",
  "dependencies": {
    "electron-updater": "^4.3.8",
    "exceljs": "^4.2.1",
    "npm": "^6.14.13",
    "user": "^0.0.0"
  },
  "devDependencies": {
    "electron": "^9.4.4",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.2.0"
  }
}
